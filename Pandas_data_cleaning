    "\n",
    "\n",
    "string_columns = df.select_dtypes(include='object').columns\n",
    "df[string_columns] = df[string_columns].apply(lambda x: x.str.strip())\n",
    "df[string_columns] = df[string_columns].apply(lambda x: x.str.capitalize())"
   ]
